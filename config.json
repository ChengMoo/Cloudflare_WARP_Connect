{
"inbound": {
    "protocol": "vmess",
    "listen": "127.0.0.1",
 "port": xxxxx,//输入本地监听端口号
 "settings": {"clients": [
         {
       "id": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"//输入uuid
          }
]},
 "streamSettings": {
 "network": "ws",
 "wsSettings": {"path": "/xxx"}//输入路径
    }
},
 "outbounds": [
    {
      "tag":"A",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag":"AAAA",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv6"
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "outboundTag": "AAAA",
        "domain": ["geosite:netflix"]
      },
      {
        "type": "field",
        "outboundTag": "A",
        "network": "udp,tcp"
      }
    ]
  }
}
}

