//xray替换位置: /usr/local/etc/xray
//v2ray替换位置: /usr/local/etc/v2ray
{
"inbound": {
    "protocol": "vmess",
    "listen": "127.0.0.1",
 "port": xxxxx,//输入本地监听端口号
 "settings": {"clients": [
         {
       "id": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"//输入uuid
          }
]},
 "streamSettings": {
 "network": "ws",
 "wsSettings": {"path": "/xxx"}//输入路径
    }
},
"outbounds": [
    {
      "tag":"A",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4"
      }
    },
    {
      "tag":"AAAA",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv6"
      }
    }
  ],
"routing": {
    "rules": [
      {
        "type": "field",
        "outboundTag": "A",
        "domain": ["geosite:netflix","nflxvideo.net","nflxext.com","nflxso.net"]
      },
      {
        "type": "field",
        "outboundTag": "AAAA",
        "network": "udp,tcp"
      }
    ]
  }
}
}

